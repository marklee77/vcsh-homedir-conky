[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/conky.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-conky.git conky
setpush  = vcsh conky remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-conky.git
           vcsh conky remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-conky.git
           vcsh conky remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-conky.git

