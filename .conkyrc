background yes

alignment top_right
gap_x 25
gap_y 50

own_window yes
own_window_type override
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager

uppercase no
default_color 657b83
double_buffer yes
draw_borders no
draw_outline no
draw_shades no
use_xft yes
xftalpha 0.8
xftfont DejaVu Sans Mono for Powerline-11:style=Book

update_interval 300
text_buffer_size 10240

